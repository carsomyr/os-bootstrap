<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<!-- This file is managed by Chef. Your changes will be overwritten. -->
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>homebrew.postgresql.server</string>
    <key>ProgramArguments</key>
    <array>
        <string><%= (prefix + "opt/postgresql/bin/postgres").to_s.encode(xml: :text) -%></string>
        <string>-D</string>
        <string><%= (prefix + "var/postgres").to_s.encode(xml: :text) -%></string>
        <string>-r</string>
        <string><%= (prefix + "var/postgres/server.log").to_s.encode(xml: :text) -%></string>
    </array>
    <key>WorkingDirectory</key>
    <string><%= (prefix + "var/postgres").to_s.encode(xml: :text) -%></string>
    <key>StandardErrorPath</key>
    <string><%= (prefix + "var/postgres/server.log").to_s.encode(xml: :text) -%></string>
    <key>UserName</key>
    <string><%= owner -%></string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
</dict>
</plist>
